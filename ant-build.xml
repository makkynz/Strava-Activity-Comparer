  <!-- Project Libraries -->
  
  <path id="library.commons-beanutils:commons-beanutils:1.9.3.classpath">
    <pathelement location="${basedir}/lib/commons-beanutils-1.9.3.jar"/>
    <pathelement location="${basedir}/lib/commons-collections-3.2.2.jar"/>
    <pathelement location="${basedir}/lib/commons-logging-1.2.jar"/>
  </path>
  
  <path id="library.lib.classpath">
    <fileset dir="${basedir}/lib">
      <patternset refid="library.patterns"/>
    </fileset>
  </path>
  
  <path id="library.org.apache.commons:commons-configuration2:2.7.classpath">
    <pathelement location="${basedir}/lib/commons-configuration2-2.7.jar"/>
