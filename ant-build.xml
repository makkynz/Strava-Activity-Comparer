    <property name="temp.jar.path.StravaCustom.jar" value="${artifacts.temp.dir}/StravaCustom.jar"/>
    <property name="temp.jar.path.StravaCustom_war.war" value="${artifacts.temp.dir}/StravaCustom_war.war"/>
  </target>
  
  <target name="clean.artifact.stravacustom:war_exploded" description="clean StravaCustom:war exploded artifact output">
